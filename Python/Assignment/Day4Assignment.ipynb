   "source": [
    "for num in range(1042000,702648265 + 1):\n",
    "    sum = 0\n",
    "#     print(num)\n",
    "    temp = num\n",
    "    while temp > 0 :\n",
    "        digit = temp%10\n",
    "        sum += digit ** 3\n",
    "        temp //= 10\n",
    "#         print(temp)\n",
    "#     print(num, sum)\n",
    "    if(num == sum):\n",
    "        print(num)"
   ]
